{
  "name": "worldgen",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:deploy": "pnpm build && pnpm generate && rm -R dist/server",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "generate": "pnpm build:server && node prerender.mjs",
    "format": "dprint fmt",
    "lint": "eslint src/main/** src/test/**",
    "serve": "vite preview",
    "test": "vitest",
    "test:load": "vite build -l warn --ssr src/tryLoadVersion.ts --outDir dist/test && node dist/test/tryLoadVersion.mjs"
  },
  "dependencies": {
    "@delangle/use-modal": "^1.9.41",
    "fflate": "^0.7.4",
    "file-saver": "^2.0.5",
    "idb-keyval": "^6.2.1",
    "json-view-for-react": "^2.0.1",
    "leaflet": "^1.9.3",
    "nanoid": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-hot-toast": "^2.4.1",
    "react-select": "^5.7.3",
    "react-window": "^1.8.9",
    "use-local-storage-state": "^18.3.2"
  },
  "devDependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/file-saver": "^2.0.5",
    "@types/leaflet": "^1.9.3",
    "@types/node": "^20.2.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-window": "^1.8.5",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@vitejs/plugin-react": "^4.0.0",
    "chalk": "^5.2.0",
    "dotenv": "^16.0.3",
    "dprint": "^0.36.1",
    "esbuild": "^0.17.19",
    "eslint": "^8.40.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^22.0.0",
    "minecraft-data": "^3.33.0",
    "react-select-event": "^5.5.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.62.1",
    "typescript": "~5.0.4",
    "vite": "^4.3.8",
    "vitest": "^0.31.1"
  }
}
