{
  "name": "worldgen",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:deploy": "pnpm build && pnpm generate && rm -R dist/server",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "generate": "pnpm build:server && node prerender.js",
    "format": "dprint fmt",
    "lint": "eslint src/main/** src/test/**",
    "serve": "vite preview",
    "test": "vitest",
    "test:load": "vite build -l warn --ssr src/tryLoadVersion.ts --outDir dist/test && node dist/test/tryLoadVersion.js"
  },
  "dependencies": {
    "@delangle/use-modal": "^1.9.41",
    "fflate": "^0.8.2",
    "file-saver": "^2.0.5",
    "idb-keyval": "^6.2.1",
    "json-view-for-react": "^2.0.1",
    "leaflet": "^1.9.4",
    "nanoid": "^5.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-hot-toast": "^2.4.1",
    "react-select": "^5.8.0",
    "react-window": "^1.8.10",
    "use-local-storage-state": "^19.1.0"
  },
  "devDependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/file-saver": "^2.0.7",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.4",
    "dprint": "^0.45.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^24.0.0",
    "react-select-event": "^5.5.1",
    "sass": "^1.70.0",
    "tslib": "^2.6.2",
    "typescript": "~5.3.3",
    "vite": "^5.1.2",
    "vitest": "^1.2.2"
  }
}
